    <table class="table">
        <thead class="thead-dark">
          <tr>
            <th scope="col">#</th>
            <th scope="col">Author</th>
            <th scope="col">Title</th>
            <th scope="col">Date</th>
          </tr>
        </thead>
        <tbody>
            <% for(var i = 0; i < 결과.length; i++){ %> 
                <tr>
                    <th scope="row"><%= i+1 %></th>
                    <td><%= 결과[i].author %></td>
                    <td><%= 결과[i].title %></td>
                    <td><%= 결과[i].date %></td>
                </tr>
            <% } %>
        </tbody>
      </table>
      
      </table>
    
